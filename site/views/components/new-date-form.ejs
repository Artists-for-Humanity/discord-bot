<form action="/add-date" method="POST">
  <fieldset>
    <label>Name</label>
    <input type="text" name="name" required />
  </fieldset>
  <fieldset>
    <label>Date</label>
    <input type="date" name="date" required/>
  </fieldset>
  <fieldset>
    <button type="submit">Add Date</button>
  </fieldset>
</form>
